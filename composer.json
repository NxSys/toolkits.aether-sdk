        "react/zmq":                    "^0.3.0",
        "sensiolabs/ansi-to-html":      "~1.1",
        "symfony/config":               "~3.0",
        "symfony/console":              "~3.0",
        "symfony/dependency-injection": "~3.0",
        "symfony/process":              "~3.0",
        "symfony/security-acl":         "~3.0",
        "symfony/security-core":        "~3.0"
